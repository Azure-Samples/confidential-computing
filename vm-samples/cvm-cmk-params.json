        "value": "<YOUR_PASSWORD>"
      },
      "vmName": {
        "value": "<YOUR_VM_NAME>"
      },
      "diskEncryptionSetId": {
        "value": "/subscriptions/<YOUR_RESOURCE_ID/resourceGroups/<YOUR_RESOURCE_GROUP>/providers/Microsoft.Compute/diskEncryptionSets/<YOUR_DES_NAME>"
      }
    }
  }